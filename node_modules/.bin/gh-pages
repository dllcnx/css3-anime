#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/dllcnx/jukaifeng/Workplace/dllcnx/code/css3-anime/node_modules/.pnpm/gh-pages@6.1.1/node_modules/gh-pages/bin/node_modules:/Users/dllcnx/jukaifeng/Workplace/dllcnx/code/css3-anime/node_modules/.pnpm/gh-pages@6.1.1/node_modules/gh-pages/node_modules:/Users/dllcnx/jukaifeng/Workplace/dllcnx/code/css3-anime/node_modules/.pnpm/gh-pages@6.1.1/node_modules:/Users/dllcnx/jukaifeng/Workplace/dllcnx/code/css3-anime/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/dllcnx/jukaifeng/Workplace/dllcnx/code/css3-anime/node_modules/.pnpm/gh-pages@6.1.1/node_modules/gh-pages/bin/node_modules:/Users/dllcnx/jukaifeng/Workplace/dllcnx/code/css3-anime/node_modules/.pnpm/gh-pages@6.1.1/node_modules/gh-pages/node_modules:/Users/dllcnx/jukaifeng/Workplace/dllcnx/code/css3-anime/node_modules/.pnpm/gh-pages@6.1.1/node_modules:/Users/dllcnx/jukaifeng/Workplace/dllcnx/code/css3-anime/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../gh-pages/bin/gh-pages.js" "$@"
else
  exec node  "$basedir/../gh-pages/bin/gh-pages.js" "$@"
fi
